<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <ui:VisualElement name="content" class="box" style="justify-content: flex-start;">
        <Style src="/Packages/com.nfu.resourcemanager/Editor/USS/ResourceComparerWindow.uss" />
        <uie:Toolbar style="display: none;">
            <uie:ToolbarButton text="All (0)" name="toolbar_all" />
            <uie:ToolbarButton text="Added (0)" name="toolbar_added" />
            <uie:ToolbarButton text="Changed (0)" name="toolbar_changed" />
            <uie:ToolbarButton text="Removed (0)" name="toolbar_removed" />
            <uie:ToolbarButton text="Expand" name="toolbar_expand" />
        </uie:Toolbar>
        <ui:VisualElement name="list_box" style="overflow: visible; display: flex;">
            <ui:ListView focusable="true" name="list" item-height="20" />
